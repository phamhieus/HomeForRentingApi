        <member name="M:AspImp.Controllers.AuthenticationController.RegisterUser(Data.DTO.UserForRegistrationDto)">
            <summary>
            register api
            </summary>
            <remarks>
            Sample request:
            
             POST /api/authentication/register
             {
                "firstname": "Jonh",
                "lastname": "Doe",
                "username": "JDoe",
                "email": "phamhieutb.dev@gmail.com",
                "password": "Password100;",
                "phonenumber": "33333",
                "roles": [ "User"], //Default will be user
              }
              
            </remarks>
            <param name="userRegistry"></param>
            <returns></returns>
            <response code="201">if user is valid and created</response>
            <response code="400">If any property is not correct</response>  
        </member>
